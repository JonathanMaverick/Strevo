    "@ic-reactor/react": "^1.14.6",
    "@supabase/supabase-js": "^2.55.0",
    "@tailwindcss/vite": "^4.1.11",
    "axios": "^1.11.0",
    "framer-motion": "^12.23.12",
    "hls.js": "^1.6.10",
    "lucide-react": "^0.539.0",
